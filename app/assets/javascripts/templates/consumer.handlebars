<h1>Consumer</h1>
{{#if model}}
  <table>
  <tr>
  <th>id</th>
  <th>name</th>
  <th>webhooks</th>
  <th> </th>
  </tr>
  <tr>
  <td>{{model.id}}</td>
  <td>{{model.name}}</td>
  <td>{{model.webhook_count}}</td>
  <td><a class="btn btn-danger" {{action destroyRecord}}>delete</a></td>
  </tr>
  </table>
  <h2>Webhooks</h2>
  {{#each webhook in model.webhooks}}
    {{#linkTo "webhook" webhook}}
      {{webhook.created_at}} - {{webhook.failed}}<br>
    {{/linkTo}}
  {{else}}
    No webhooks for this consumer matey
  {{/each}}

{{else}}
  Something went wrong.
{{/if}}
